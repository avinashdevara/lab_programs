first=int(input("Enter lower range: "))  
last=int(input("Enter upper range: "))  
  
for n in range(first,last+1):  
   if n>1:  
       for i in range(2,n):  
           if (n%i)==0:  
               break  
       else:  
           print(n)  
           
           
           
   OUTPUT:
   Enter lower range:  2
Enter upper range:  100
